{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "tiny chat",
      "type": "python",
      "request": "launch",
      "module": "swarm.chat",
      "args": [
        "-m",
        "PygmalionAI/pygmalion-350m"
      ],
      "cwd": "${workspaceFolder}/",
      "justMyCode": true
    },
    {
      "name": "small chat",
      "type": "python",
      "request": "launch",
      "module": "swarm.chat",
      "args": [
        "-m",
        "PygmalionAI/pygmalion-1.3b"
      ],
      "cwd": "${workspaceFolder}/",
      "justMyCode": true
    },
    {
      "name": "medium chat",
      "type": "python",
      "request": "launch",
      "module": "swarm.chat",
      "args": [
        "-m",
        "bigscience/bloomz-3b"
      ],
      "cwd": "${workspaceFolder}/",
      "justMyCode": true
    },
    {
      "name": "large chat",
      "type": "python",
      "request": "launch",
      "module": "swarm.chat",
      "args": [
        "-m",
        "bigscience/bloomz-7b1",
        "-d",
        "cpu"
      ],
      "cwd": "${workspaceFolder}/",
      "justMyCode": true
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "PowerShell Launch Current File",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "cwd": "${cwd}"
    },
    {
      "name": "Worker",
      "type": "python",
      "request": "launch",
      "module": "swarm.worker",
      "cwd": "${workspaceFolder}/",
      "justMyCode": true
    },
    {
      "name": "Init",
      "type": "python",
      "request": "launch",
      "module": "swarm.initialize",
      "cwd": "${workspaceFolder}/",
      "justMyCode": true
    },
    {
      "name": "test",
      "type": "python",
      "request": "launch",
      "module": "swarm.test",
      "cwd": "${workspaceFolder}/",
      "justMyCode": true
    }
  ]
}